{
  "name": "slack-server",
  "version": "1.0.0",
  "description": "Slack micro-service",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/server.js\"",
    "build": "babel src --copy-files --source-maps=true --out-dir dist",
    "start": "node dist/server.js",
    "prestart": "npm run -s build",
    "test": "eslint",
    "lint": "eslint ./src --fix"
  },
  "author": "OSTEP Team",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@slack/client": "^4.11.0",
    "@slack/events-api": "^2.1.1",
    "axios": "^0.18.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "luxon": "^1.11.4",
    "morgan": "^1.8.0",
    "request": "^2.88.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  }
}
